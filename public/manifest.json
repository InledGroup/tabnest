{
  "manifest_version": 3,
  "name": "TabNest",
  "version": "1.0",
  "description": "Nueva pesta√±a personalizada con marcadores y noticias.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "alarms",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "tabnest.png",
    "48": "tabnest.png",
    "128": "tabnest.png"
  }
}